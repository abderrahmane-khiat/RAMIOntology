@prefix : <https://w3id.org/i40/rami#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix om: <http://www.wurvoc.org/vocabularies/om-1.8/> .
@prefix DUL: <http://www.loa-cnr.it/ontologies/DUL.owl#> .
@prefix geo: <http://www.w3.org/2003/01/geo/wgs84_pos#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix ssn: <http://purl.oclc.org/NET/ssnx/ssn#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix foaf: <http://xmlns.com/foaf/spec/> .
@prefix obda: <https://w3id.org/obda/vocabulary#> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix rami: <https://w3id.org/i40/rami#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix vann: <http://purl.org/vocab/vann/> .
@prefix void: <http://rdfs.org/ns/void#> .
@prefix vcard: <http://www.w3.org/2006/vcard/ns#> .
@prefix schema: <http://www.w3.org/2001/XMLSchema#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@base <https://w3id.org/i40/rami#> .

<https://w3id.org/i40/rami#> rdf:type owl:Ontology ;
                              void:vocabulary foaf: ;
                              vann:preferredNamespaceUri "https://w3id.org/i40/rami#" ;
                              void:vocabulary rdf: ;
                              skos:definition "An Ontology to represents the Reference Architecture Model for Industry 4.0 (RAMI), including the concept of the Administration Shell I4.0 Component."@en ;
                              void:vocabulary rdfs: ,
                                              skos: ,
                                              dc: ,
                                              dcterms: ;
                              skos:prefLabel "rami"@en ;
                              dcterms:created "2015-11-30"^^schema:date ;
                              owl:versionInfo "0.1" ;
                              void:vocabulary schema: ;
                              dcterms:publisher <http://de.dbpedia.org/resource/Fraunhofer-Gesellschaft> ;
                              vann:preferredNamespacePrefix "rami" ;
                              void:vocabulary xml: ;
                              rdfs:isDefinedBy <https://w3id.org/i40/rami#> ;
                              dcterms:creator <http://sda.cs.uni-bonn.de/Irlan/> .

#################################################################
#    Annotation properties
#################################################################

###  http://purl.org/dc/elements/1.1/identifier
dc:identifier rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/elements/1.1/source
dc:source rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/identifier
dcterms:identifier rdf:type owl:AnnotationProperty ;
                   rdfs:subPropertyOf dc:identifier ;
                   rdfs:range rdfs:Literal ;
                   rdfs:domain rami:BasicData .


###  http://purl.org/dc/terms/relation
dcterms:relation rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/source
dcterms:source rdf:type owl:AnnotationProperty ;
               rdfs:comment "A related resource from which the described resource is derived."@en ;
               rdfs:isDefinedBy dcterms: ;
               rdfs:label "Source"@en ;
               skos:note "This term is intended to be used with non-literal values as defined in the DCMI Abstract Model (http://dublincore.org/documents/abstract-model/).  As of December 2007, the DCMI Usage Board is seeking a way to express this intention with a formal range declaration."@en ;
               rdfs:subPropertyOf dc:source ,
                                  dcterms:relation .


###  http://www.w3.org/2004/02/skos/core#altLabel
skos:altLabel rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2004/02/skos/core#definition
skos:definition rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2004/02/skos/core#example
skos:example rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2004/02/skos/core#hiddenLabel
skos:hiddenLabel rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2004/02/skos/core#note
skos:note rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2004/02/skos/core#prefLabel
skos:prefLabel rdf:type owl:AnnotationProperty .


#################################################################
#    Datatypes
#################################################################

###  http://www.w3.org/2001/XMLSchema#date
schema:date rdf:type rdfs:Datatype .


#################################################################
#    Object Properties
#################################################################

###  https://w3id.org/i40/rami#contributes
rami:contributes rdf:type owl:ObjectProperty ;
                 rdfs:domain rami:BasicData ;
                 rdfs:range prov:Agent ;
                 skos:definition "Agent(Person or Software) that contributes to the Entity/Object related data. It correspond to the Contributor concept of OMM"@en ;
                 skos:prefLabel "has Type Description"@en .


###  https://w3id.org/i40/rami#createdBy
rami:createdBy rdf:type owl:ObjectProperty ;
               rdfs:domain rami:BasicData ;
               rdfs:range prov:Agent ;
               skos:definition "Agent (Person or Software) that creates to the Entity/Object related data. It correspond to the Creator concept of OMM"@en ;
               skos:prefLabel "has Creator"@en .


###  https://w3id.org/i40/rami#hasBrakingResistance
rami:hasBrakingResistance rdf:type owl:ObjectProperty ;
                          rdfs:domain rami:ElectricalData .


###  https://w3id.org/i40/rami#hasData
rami:hasData rdf:type owl:ObjectProperty ;
             rdfs:domain rami:AdminShell ;
             rdfs:range rami:BasicData ;
             skos:definition "Connects the AdminShell with the different types of Data for the Object/Entity"@en ;
             skos:prefLabel "has Data"@en .


###  https://w3id.org/i40/rami#hasInputVoltage
rami:hasInputVoltage rdf:type owl:ObjectProperty ;
                     rdfs:domain rami:ElectricalData .


###  https://w3id.org/i40/rami#hasRAMIHierarchyLevel
rami:hasRAMIHierarchyLevel rdf:type owl:ObjectProperty ;
                           rdfs:range rami:RAMIHierarchyLevel ;
                           skos:definition "Connects the concepts with the correspondent layer in the RAMI Hierarchy Levels"@en ;
                           skos:prefLabel "has RAMI Hierarchy Level" .


###  https://w3id.org/i40/rami#hasRAMILifeCycleLayer
rami:hasRAMILifeCycleLayer rdf:type owl:ObjectProperty ;
                           rdfs:range rami:RAMILayer ;
                           skos:definition "Connects the concepts with the correspondent layer in the RAMI Hierarchy Levels"@en ;
                           skos:prefLabel "has RAMI Life Cycle Layer" .


###  https://w3id.org/i40/rami#hasTechnicalFuncionality
rami:hasTechnicalFuncionality rdf:type owl:ObjectProperty ;
                              rdfs:domain rami:AdminShell ;
                              rdfs:range rami:TechnicalFunctionality ;
                              skos:definition "The Administrative Shell has Technical Funcionalities"@en ;
                              skos:prefLabel "has Technical Funcionality"@en .


###  https://w3id.org/i40/rami#isDescribedWith
rami:isDescribedWith rdf:type owl:ObjectProperty ;
                     rdfs:domain rami:AdminShell ;
                     rdfs:range rami:Standard .


###  https://w3id.org/i40/rami#isPartOf
rami:isPartOf rdf:type owl:ObjectProperty ,
                       owl:TransitiveProperty ;
              rdfs:domain rami:Asset ;
              rdfs:range rami:Asset ;
              rdfs:comment "The Object can be part of of other Objects"@en ;
              rdfs:label "isPartOf"@en .


###  https://w3id.org/i40/rami#outputFrequency
rami:outputFrequency rdf:type owl:ObjectProperty ;
                     rdfs:domain rami:ElectricalData .


###  https://w3id.org/i40/rami#publishedURL
rami:publishedURL rdf:type owl:ObjectProperty ;
                  rdfs:domain rami:Standard .


###  https://w3id.org/i40/rami#wraps
rami:wraps rdf:type owl:ObjectProperty ;
           rdfs:domain rami:AdminShell ;
           rdfs:range rami:Asset ;
           skos:definition "Connect the Object with the Admin Shell"@en ;
           skos:prefLabel "wraps"@en .


#################################################################
#    Data properties
#################################################################

###  http://purl.org/dc/terms/identifier
dcterms:identifier rdf:type owl:DatatypeProperty .


###  http://www.w3.org/2003/01/geo/wgs84_pos#lat
geo:lat rdf:type owl:DatatypeProperty ;
        rdfs:domain rami:Machine ;
        rdfs:range schema:decimal ;
        rdfs:comment "The WGS84 altitude of a SpatialThing (decimal meters above the local reference ellipsoid)."@en ;
        rdfs:label "lat"@en .


###  http://www.w3.org/2003/01/geo/wgs84_pos#long
geo:long rdf:type owl:DatatypeProperty ;
         rdfs:domain rami:Machine ;
         rdfs:range schema:decimal ;
         rdfs:comment "The WGS84 longitude of a SpatialThing (decimal degrees)."@en ;
         rdfs:label "long"@en .


###  https://w3id.org/i40/rami#CADFormat
rami:CADFormat rdf:type owl:DatatypeProperty ;
               rdfs:domain rami:MCAD ;
               rdfs:range schema:string ;
               skos:definition "Different types of format files for CAD Archive of the Object/Entity"@en ;
               skos:prefLabel "CAD Format"@en .


###  https://w3id.org/i40/rami#CADSystem
rami:CADSystem rdf:type owl:DatatypeProperty ;
               rdfs:domain rami:MCAD ;
               rdfs:range schema:string ;
               skos:definition "Points to the different types of CAD systems of the Object/Entity"@en ;
               skos:example "Neutral 2D, Neutral 3D, SolidWorks" ;
               skos:prefLabel "CAD Format"@en .


###  https://w3id.org/i40/rami#CADarchive
rami:CADarchive rdf:type owl:DatatypeProperty ;
                rdfs:domain rami:MCAD ;
                rdfs:range schema:anyURI ;
                skos:definition "Link(URL) to the CAD Archive of the Object/Entity"@en ;
                skos:prefLabel "CAD Archive"@en .


###  https://w3id.org/i40/rami#blockUrl
rami:blockUrl rdf:type owl:DatatypeProperty ;
              rdfs:domain rami:Firmware ;
              rdfs:range schema:anyURI ;
              skos:definition "URL of the Firmware that belongs to the Object/Entity"@en ;
              skos:prefLabel "Block Url"@en .


###  https://w3id.org/i40/rami#certificateName
rami:certificateName rdf:type owl:DatatypeProperty ,
                              owl:FunctionalProperty ;
                     rdfs:domain rami:SafetyConfiguration ;
                     rdfs:range schema:string ;
                     skos:definition "Certificate Safety Name of the Object/Entity"@en ;
                     skos:prefLabel "Certificate Name"@en .


###  https://w3id.org/i40/rami#certificateUrl
rami:certificateUrl rdf:type owl:DatatypeProperty ,
                             owl:FunctionalProperty ;
                    rdfs:domain rami:SafetyConfiguration ;
                    rdfs:range schema:anyURI ;
                    skos:definition "Certificate Safety Url of the Object/Entity"@en ;
                    skos:prefLabel "Certificate Url"@en .


###  https://w3id.org/i40/rami#creationDate
rami:creationDate rdf:type owl:DatatypeProperty ;
                  rdfs:domain rami:BasicData ;
                  rdfs:range schema:dateTime ;
                  skos:definition "Date when an Agent(Person or Software) creates to the Entity/Object related data. It correspond to the Creator concept of OMM"@en ;
                  skos:prefLabel "has Creator"@en .


###  https://w3id.org/i40/rami#description
rami:description rdf:type owl:DatatypeProperty ;
                 rdfs:domain rami:Asset ;
                 rdfs:range schema:string ;
                 skos:definition "Description of the Object/Entity"@en ;
                 skos:prefLabel "has Description"@en .


###  https://w3id.org/i40/rami#firmwareDate
rami:firmwareDate rdf:type owl:DatatypeProperty ;
                  rdfs:domain rami:Firmware ;
                  rdfs:range schema:date ;
                  skos:definition "Firmware date that belongs to the Object/Entity"@en ;
                  skos:prefLabel "firmware Date"@en .


###  https://w3id.org/i40/rami#hasBrakingResistance
rami:hasBrakingResistance rdf:type owl:DatatypeProperty .


###  https://w3id.org/i40/rami#hasEnergyValue
rami:hasEnergyValue rdf:type owl:DatatypeProperty ;
                    rdfs:domain rami:CumulativeEnergyConsumption ;
                    rdfs:range schema:float ;
                    skos:prefLabel "hasEnergyValue"@en .


###  https://w3id.org/i40/rami#hasInputVoltage
rami:hasInputVoltage rdf:type owl:DatatypeProperty .


###  https://w3id.org/i40/rami#hasSensorId
rami:hasSensorId rdf:type owl:DatatypeProperty ,
                          owl:FunctionalProperty ;
                 rdfs:domain ssn:Sensor ;
                 rdfs:range schema:string ;
                 skos:definition "Unique Identification of the Sensor"@en ;
                 skos:prefLabel "hasSensorId"@en .


###  https://w3id.org/i40/rami#inventoryNumber
rami:inventoryNumber rdf:type owl:DatatypeProperty ,
                              owl:FunctionalProperty ;
                     rdfs:domain rami:BasicData ;
                     rdfs:range schema:string ;
                     skos:definition "Inventory number of the Entity/Object according to the identification nomenclature"@en ;
                     skos:prefLabel "Inventory Number"@en .


###  https://w3id.org/i40/rami#isDescribedWith
rami:isDescribedWith rdf:type owl:DatatypeProperty .


###  https://w3id.org/i40/rami#logs
rami:logs rdf:type owl:DatatypeProperty ;
          rdfs:range schema:string ;
          skos:prefLabel "logs"@en .


###  https://w3id.org/i40/rami#manual
rami:manual rdf:type owl:DatatypeProperty ;
            rdfs:domain rami:ApplicationData ;
            rdfs:range schema:anyURI ;
            skos:definition "Points to the Manual document of a given Object/Entity"@en ;
            skos:prefLabel "Manual"@en .


###  https://w3id.org/i40/rami#name
rami:name rdf:type owl:DatatypeProperty ,
                   owl:FunctionalProperty ;
          rdfs:domain rami:Asset ;
          rdfs:range schema:string ;
          skos:definition "Name of the Object/Entity"@en ;
          skos:prefLabel "has Name"@en .


###  https://w3id.org/i40/rami#objectId
rami:objectId rdf:type owl:DatatypeProperty ,
                       owl:FunctionalProperty ;
              rdfs:domain rami:Asset ;
              rdfs:range schema:string ;
              skos:definition "Unique ID for the Object/Entity"@en ;
              skos:prefLabel "has Object Id"@en .


###  https://w3id.org/i40/rami#outputFrequency
rami:outputFrequency rdf:type owl:DatatypeProperty .


###  https://w3id.org/i40/rami#phasesNumber
rami:phasesNumber rdf:type owl:DatatypeProperty ;
                  rdfs:domain rami:ElectricalData ;
                  rdfs:range schema:string ;
                  skos:example "Single phase, three phase" ;
                  skos:prefLabel "Phases Number"@en .


###  https://w3id.org/i40/rami#publishedURL
rami:publishedURL rdf:type owl:DatatypeProperty .


###  https://w3id.org/i40/rami#receivedTime
rami:receivedTime rdf:type owl:DatatypeProperty ;
                  rdfs:domain rami:Machine ;
                  rdfs:range schema:dateTimeStamp ;
                  skos:definition "received Time of a Machine"@en ;
                  skos:prefLabel "received Time"@en .


###  https://w3id.org/i40/rami#recordedTime
rami:recordedTime rdf:type owl:DatatypeProperty ;
                  rdfs:domain rami:Machine ;
                  rdfs:range schema:dateTimeStamp ;
                  rdfs:comment "Recorded Time for a Machine"@en ;
                  skos:prefLabel "recorded Time"@en .


###  https://w3id.org/i40/rami#releaseDate
rami:releaseDate rdf:type owl:DatatypeProperty ;
                 rdfs:domain rami:Standard ;
                 rdfs:range schema:date ;
                 skos:definition "Release Date of the Standard"@en ;
                 skos:prefLabel "Release Date"@en .


###  https://w3id.org/i40/rami#safetyName
rami:safetyName rdf:type owl:DatatypeProperty ;
                rdfs:domain rami:SafetyConfiguration ;
                rdfs:range schema:string ;
                skos:definition "Name of the Safety Configuration of the Object/Entity"@en ;
                skos:prefLabel "Safety Name"@en .


###  https://w3id.org/i40/rami#typeDescription
rami:typeDescription rdf:type owl:DatatypeProperty ;
                     rdfs:domain rami:BasicData ;
                     rdfs:range schema:string ;
                     rdfs:seeAlso <https://www.w3.org/2005/Incubator/omm/XGR-omm-20111026/> ;
                     skos:definition "Describes the Entity/Object related data. It correspond to the Description concept of OMM"@en ;
                     skos:prefLabel "type Description"@en .


###  https://w3id.org/i40/rami#version
rami:version rdf:type owl:DatatypeProperty ;
             rdfs:domain rami:Firmware ,
                         rami:Standard ;
             rdfs:range schema:string ;
             skos:definition "Current version of the Standard"@en ,
                             "Version of the Firmware that belongs to the Object/Entity"@en ;
             skos:prefLabel "Version"@en ,
                            "has Version"@en .


#################################################################
#    Classes
#################################################################

###  http://purl.oclc.org/NET/ssnx/ssn#Sensor
ssn:Sensor rdf:type owl:Class .


###  http://purl.oclc.org/NET/ssnx/ssn#SensorOutput
ssn:SensorOutput rdf:type owl:Class ;
                 owl:equivalentClass rami:SensorMeasurementData .


###  http://www.w3.org/ns/prov#Agent
prov:Agent rdf:type owl:Class .


###  http://xmlns.com/foaf/spec/Image
foaf:Image rdf:type owl:Class .


###  https://w3id.org/i40/rami#AdminShell
rami:AdminShell rdf:type owl:Class ;
                dc:source "This definition comprises material from RAMI4.0."@en ;
                skos:altLabel "Administration Shell"@en ;
                skos:definition "Describes the Administration Shell for Assets, Products, Components, e.g. Machines"@en ;
                skos:prefLabel "Admin Shell"@en ,
                               "Verwaltungsschale"@de .


###  https://w3id.org/i40/rami#AdminShellSubModel
rami:AdminShellSubModel rdf:type owl:Class .


###  https://w3id.org/i40/rami#ApplicationData
rami:ApplicationData rdf:type owl:Class ;
                     rdfs:subClassOf rami:BasicData ;
                     skos:definition "Hier bestimmt der Gegenstand das Format"@de ,
                                     "The object determines the format"@en ;
                     skos:prefLabel "Anwendungsdaten"@de ,
                                    "Application Data"@en .


###  https://w3id.org/i40/rami#Asset
rami:Asset rdf:type owl:Class ;
           owl:equivalentClass [ rdf:type owl:Class ;
                                 owl:oneOf ( rami:Instance
                                             rami:Type
                                           )
                               ] .


###  https://w3id.org/i40/rami#BasicData
rami:BasicData rdf:type owl:Class ;
               skos:definition "Describe the different types of Data related to the I4.0 Entity/Object"@en ;
               skos:prefLabel "Data"@en ,
                              "Daten"@de .


###  https://w3id.org/i40/rami#CumulativeEnergyConsumption
rami:CumulativeEnergyConsumption rdf:type owl:Class .


###  https://w3id.org/i40/rami#ElectricalData
rami:ElectricalData rdf:type owl:Class ;
                    rdfs:subClassOf rami:BasicData ;
                    skos:definition "Electrical Data for the Object"@en ;
                    skos:prefLabel "Electrical Data"@en .


###  https://w3id.org/i40/rami#EngineeringData
rami:EngineeringData rdf:type owl:Class ;
                     rdfs:subClassOf rami:BasicData ;
                     rdfs:isDefinedBy <http://www.businessdictionary.com/definition/engineering-data.html#ixzz45Ydk71Su> ;
                     skos:definition "Documents such as drawings, manufacturer's specifications, standards, and other information relating to design, procurement, fabrication, test, and inspection of an item or structure."@en ;
                     skos:prefLabel "Engineering Data"@en ,
                                    "Engineering-Daten"@de .


###  https://w3id.org/i40/rami#Firmware
rami:Firmware rdf:type owl:Class ;
              rdfs:subClassOf rami:ApplicationData ;
              skos:definition "Eingesetzte Firmware, Firmware-Module"@de ,
                              "Used firmware, firmware modules"@en ;
              skos:prefLabel "Firmware"@de ,
                             "Firmware"@en .


###  https://w3id.org/i40/rami#MCAD
rami:MCAD rdf:type owl:Class ;
          rdfs:subClassOf rami:EngineeringData ;
          skos:definition "Data that contain the files used for Mechanical Computer Aid Design of a given object"@en ;
          skos:prefLabel "MCAD"@de ,
                         "MCAD"@en .


###  https://w3id.org/i40/rami#Machine
rami:Machine rdf:type owl:Class ;
             rdfs:subClassOf rami:Asset ;
             skos:definition "A machine is ordered, designed, commissioned, operated, serviced, converted and recycled."@en ;
             skos:prefLabel "Machine"@en .


###  https://w3id.org/i40/rami#Order
rami:Order rdf:type owl:Class ;
           skos:definition "Each order for manufacturing runs through a life cycle and its specifics necessarily have an impact on the production facility during performance of the order"@en ;
           skos:prefLabel "Order"@en .


###  https://w3id.org/i40/rami#RAMIHierarchyLevel
rami:RAMIHierarchyLevel rdf:type owl:Class ;
                        owl:equivalentClass [ rdf:type owl:Class ;
                                              owl:oneOf ( rami:ConnectedWorld
                                                          rami:ControlDevice
                                                          rami:Enterprise
                                                          rami:FieldDevice
                                                          rami:Product
                                                          rami:Station
                                                          rami:WorkCenter
                                                        )
                                            ] ;
                        rdfs:seeAlso <http://www.zvei.org/en/subjects/Industry-40/Pages/The-Reference-Architectural-Model-RAMI-40-and-the-Industrie-40-Component.aspx> ;
                        skos:definition "It describes the functional classification of various circumstances within Industrie 4.0. For classification within a factory, this axis of the reference architecture(layers) follows the IEC 62264 Standard."@en ;
                        skos:prefLabel "RAMI Hierarchy Level"@en .


###  https://w3id.org/i40/rami#RAMILayer
rami:RAMILayer rdf:type owl:Class .


###  https://w3id.org/i40/rami#RAMILifeCycleLayer
rami:RAMILifeCycleLayer rdf:type owl:Class ;
                        owl:equivalentClass [ rdf:type owl:Class ;
                                              owl:oneOf ( rami:Asset
                                                          rami:Business
                                                          rami:Communication
                                                          rami:Functional
                                                          rami:Information
                                                          rami:Integration
                                                        )
                                            ] ;
                        rdfs:seeAlso <http://www.zvei.org/en/subjects/Industry-40/Pages/The-Reference-Architectural-Model-RAMI-40-and-the-Industrie-40-Component.aspx> ;
                        skos:definition "It describes the different Life Cycle and Value Stream layers for the RAMI model according to the IEC 62890 Standard."@en ;
                        skos:prefLabel "RAMI Layer"@en .


###  https://w3id.org/i40/rami#SafetyConfiguration
rami:SafetyConfiguration rdf:type owl:Class ;
                         rdfs:subClassOf rami:ApplicationData ;
                         skos:definition "Safety Configuration of the Object/Entity. It contains certificate of compliance that states that the object has fullfils the safety requirements according to a given standard"@en ;
                         skos:prefLabel "SafetyConfiguration"@en .


###  https://w3id.org/i40/rami#SensorMeasurementData
rami:SensorMeasurementData rdf:type owl:Class ;
                           rdfs:subClassOf rami:BasicData ;
                           rdfs:comment "Data measured by a sensor"@en ;
                           rdfs:label "Sensor Data"@en .


###  https://w3id.org/i40/rami#Standard
rami:Standard rdf:type owl:Class ;
              skos:definition "Standards that are used to describe the actual I4.0 Entity/Object"@en ;
              skos:prefLabel "Standard"@de ,
                             "Standard"@en .


###  https://w3id.org/i40/rami#TechnicalFunctionality
rami:TechnicalFunctionality rdf:type owl:Class ;
                            skos:definition "Technical Functionality of the Administration Shell"@en ;
                            skos:note "Technical functionality takes place in the Functional Layer of the reference architecture model RAMI4.0"@en ;
                            skos:prefLabel "Fachliche Funktionalität"@de ,
                                           "Technical Functionality"@en .


#################################################################
#    Individuals
#################################################################

###  https://w3id.org/i40/rami#Asset
rami:Asset rdf:type owl:NamedIndividual .


###  https://w3id.org/i40/rami#Business
rami:Business rdf:type owl:NamedIndividual .


###  https://w3id.org/i40/rami#Communication
rami:Communication rdf:type owl:NamedIndividual .


###  https://w3id.org/i40/rami#ConnectedWorld
rami:ConnectedWorld rdf:type owl:NamedIndividual ,
                             rami:RAMIHierarchyLevel ;
                    skos:definition "Connected World hierarchy level of RAMI 4.0. Group of Factories, and the collaboration with external engineering firms, component suppliers and customers"@en ;
