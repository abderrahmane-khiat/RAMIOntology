@base <https://w3id.org/i40/rami/> .


###  https://w3id.org/i40/rami#AccessControl
rami:AccessControl rdf:type owl:Class ;
                   rdfs:comment "Access Control defines the local access control policy administration point. Access Control has the major task to define the access permission rules."@en ;
                   rdfs:label "Access Control"^^xsd:string .

###  https://w3id.org/i40/rami#hasExternalAccessControl
rami:hasExternalAccessControl rdf:type owl:ObjectProperty ;
                              rdfs:comment "Endpoint to an external access control defining a policy administration point to be used by the AAS."@en ;
                              rdfs:label "has externalAccesControl"^^xsd:string .


###  https://w3id.org/i40/rami#hasPermission
rami:hasPermission rdf:type owl:ObjectProperty ;
                   rdfs:comment """Permissions assigned to the object.
The permissions hold for all subjects as specified in the access permission rule. It can be also the Reference to a property that defines the semantics of the permission."""@en ;
                   rdfs:label "hasPermission"^^xsd:string .


###  https://w3id.org/i40/rami#hasPolicyAdministrationPoint
rami:hasPolicyAdministrationPoint rdf:type owl:ObjectProperty ;
                                  rdfs:comment "The access control administration policy point of the AAS."@en ;
                                  rdfs:label "has PolicyAdministrationPoint"^^xsd:string .


###  https://w3id.org/i40/rami#hasPolicyDecisionPoint
rami:hasPolicyDecisionPoint rdf:type owl:ObjectProperty ;
                            rdfs:comment "The access control policy decision point of the AAS."@en ;
                            rdfs:label "has PolicyDecisionPoint"^^xsd:string .


###  https://w3id.org/i40/rami#hasPolicyEnforcementPoint
rami:hasPolicyEnforcementPoint rdf:type owl:ObjectProperty ;
                               rdfs:comment "The access control policy enforcement point of the AAS."@en ;
                               rdfs:label "has PolicyEnforcementPoint"^^xsd:string .

rami:hasView rdf:type owl:ObjectProperty ;
             rdfs:domain [ rdf:type owl:Class ;
                           owl:unionOf ( rami:AdminShell
                                         rami:Submodel
                                       )
                         ] ;
					
###  https://w3id.org/i40/rami#accessPermissionRule
rami:accessPermissionRule rdf:type owl:ObjectProperty ;
                          rdfs:comment "Access permission rules of the AAS describing the rights assigned to (already authenticated) subjects to access elements of the AAS."@en ;
                          rdfs:label "accessPermissionRule"^^xsd:string .



###  https://w3id.org/i40/rami#containsAS
rami:containsAS rdf:type owl:ObjectProperty ;
                rdfs:domain rami:I4.0Component ;
                rdfs:range rami:AdminShell ;
                rdfs:comment "Points the I4.0 Component to the Administration Shell"@en ;
                rdfs:label "contains Admin Shell"@en .


###  http://www.wurvoc.org/vocabularies/om-1.8/hasUnit
om:hasUnit rdf:type owl:ObjectProperty ;
           rdfs:domain om:Measure ;
           rdfs:range om:Unit ;
           rdfs:label "has unit"@en .



###  https://w3id.org/i40/rami#Structure
rami:Structure rdf:type owl:Class ;
               rdfs:subClassOf rami:Submodel .



###  https://w3id.org/i40/rami#hasEnergyValue
rami:hasEnergyValue rdf:type owl:DatatypeProperty ;
                    rdfs:domain rami:CumulativeEnergyConsumption ;
                    rdfs:range xsd:float ;
                    skos:prefLabel "has Energy Value"@en .


###  https://w3id.org/i40/rami#externalInformationPoint
rami:externalInformationPoint rdf:type owl:ObjectProperty ;
                              rdfs:comment "Endpoints to external available information points taking into Endpoint aggr 0..* consideration for access control for the AAS."@en ;
                              rdfs:label "external InformationPoint"^^xsd:string .

###  https://w3id.org/i40/rami#localAccessControl
rami:localAccessControl rdf:type owl:ObjectProperty ;
                        rdfs:comment """The policy decision point of access control as realized by the AAS itself.

Either there is an external policy administration point endpoint defined or the AAS has its own access control."""@en ;
                        rdfs:label "local AccessControl"^^xsd:string .
	 
###  https://w3id.org/i40/rami#hasPolicyInformationPoints
rami:hasPolicyInformationPoints rdf:type owl:ObjectProperty ;
                                rdfs:comment "The access control policy information points of the AAS."@en ;
                                rdfs:label "has PolicyInformationPoints"^^xsd:string .


###  https://w3id.org/i40/rami#ComponentManager
rami:ComponentManager rdf:type owl:Class ;
                      rdfs:label "Component Manager"@en .

###  https://w3id.org/i40/rami#AccessPermissionRule
rami:AccessPermissionRule rdf:type owl:Class ;
                          rdfs:comment "Table that defines access permissions per authenticated subject for a set of objects (referable elements)."@en ;
                          rdfs:label "Access Permission Rule"^^xsd:string .

###  https://w3id.org/i40/rami#Dimension
rami:Dimension rdf:type owl:Class ;
               rdfs:comment "Abstract class to describe the three different dimensions of RAMI4.0"@en ;
               rdfs:label "Dimension"@en .



###  https://w3id.org/i40/rami#hasI40ComponentId
rami:hasI40ComponentId rdf:type owl:DatatypeProperty ;
                       rdfs:subPropertyOf dcterms:identifier ;
                       rdf:type owl:FunctionalProperty ;
                       rdfs:domain rami:I4.0Component ;
                       rdfs:range xsd:string ;
                       rdfs:comment "Unique Identification of the Industry 4.0 component."@en ;
                       rdfs:label "has I40 Component Id"@en ;
                       owl:deprecated "true"^^xsd:boolean .

###  https://w3id.org/i40/rami#HierarchyLevelDimension
rami:HierarchyLevelDimension rdf:type owl:Class ;
                             owl:equivalentClass [ rdf:type owl:Class ;
                                                   owl:oneOf ( rami:ConnectedWorld
                                                               rami:ControlDevice
                                                               rami:Enterprise
                                                               rami:FieldDevice
                                                               rami:Product
                                                               rami:Station
                                                               rami:WorkCenter
                                                             )
                                                 ] ;
                             rdfs:subClassOf rami:Dimension ;
                             rdfs:seeAlso <http://www.zvei.org/en/subjects/Industry-40/Pages/The-Reference-Architectural-Model-RAMI-40-and-the-Industrie-40-Component.aspx> ;
                             skos:definition "It describes the functional classification of various circumstances within Industrie 4.0. For classification within a factory, this axis of the reference architecture(layers) follows the IEC 62264 Standard."@en ;
                             skos:prefLabel "RAMI Hierarchy Level Dimension"@en .


###  https://w3id.org/i40/rami#I4.0Component
rami:I4.0Component rdf:type owl:Class ;
                   owl:equivalentClass [ rdf:type owl:Restriction ;
                                         owl:onProperty rami:containsAS ;
                                         owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                         owl:onClass rami:AdminShell
                                       ] ;
                   rdfs:comment """Globally uniquely identifiable participant with communication capability consisting of administration shell and asset within an I4.0 system. 
Source: Industrie 4.0 – Begriffe/Terms, VDI Statusreport Industrie 4.0 (April 2017)"""@en ;
                   rdfs:isDefinedBy "https://www.plattform-i40.de/I40/Navigation/EN/Service/Glossary/glossary.html" ;
                   rdfs:label "I4.0Component"@en .


###  https://w3id.org/i40/rami#ITLayerDimension
rami:ITLayerDimension rdf:type owl:Class ;
                      owl:equivalentClass [ rdf:type owl:Class ;
                                            owl:oneOf ( rami:Asset
                                                        rami:Business
                                                        rami:Communication
                                                        rami:Functional
                                                        rami:Information
                                                        rami:Integration
                                                      )
                                          ] ;
                      rdfs:subClassOf rami:Dimension ;
                      rdfs:label "RAMI IT Layer Dimension"@en .

###  https://w3id.org/i40/rami#Permission
rami:Permission rdf:type owl:Class ;
                rdfs:comment "Description of a single permission."@en ;
                rdfs:label "Permission"^^xsd:string .


###  https://w3id.org/i40/rami#PermissionPerObject
rami:PermissionPerObject rdf:type owl:Class ;
                         rdfs:comment "Table that defines access permissions for a specified object. The object is any referable element in the AAS. Additionally object attributes can be defined that further specify the kind of object the permissions apply to."@en ;
                         rdfs:label "Permission Per Object"^^xsd:string .


###  https://w3id.org/i40/rami#Platform
rami:Platform rdf:type owl:Class .


###  https://w3id.org/i40/rami#PolicyAdministrationPoint
rami:PolicyAdministrationPoint rdf:type owl:Class ;
                               rdfs:comment "Definition of a security administration point (PDP)."@en ;
                               rdfs:label "PolicyAdministrationPoint"^^xsd:string .


###  https://w3id.org/i40/rami#PolicyInformationPoints
rami:PolicyInformationPoints rdf:type owl:Class ;
                             rdfs:comment """Defines the security policy information points (PIP).
Serves as the retrieval source of attributes, or the data required for policy evaluation to provide the information needed by the policy decision point to make the decisions."""@en ;
                             rdfs:label "Policy Information Points"^^xsd:string .

###  https://w3id.org/i40/rami#SubjectAttributes
rami:SubjectAttributes rdf:type owl:Class ;
                       rdfs:comment "A sert of data elements that further classifies a specific subject."@en ;
                       rdfs:label "Subject attributes"^^xsd:string .

###  https://w3id.org/i40/rami#TransportInterface
rami:TransportInterface rdf:type owl:Class ;
                        rdfs:subClassOf rami:Submodel .

###  https://w3id.org/i40/rami#View
rami:View rdf:type owl:Class ;
          owl:equivalentClass [ rdf:type owl:Class ;
                                owl:oneOf ( rami:BusinessView
                                            rami:Construction
                                            rami:Function
                                            rami:Location
                                            rami:Performance
                                          )
                              ] ;
          rdfs:comment "Different views associated to the Administration Shell via the Submodels"@en ;
          rdfs:isDefinedBy "https://www.plattform-i40.de/I40/Redaktion/DE/Downloads/Publikation/hm-2018-trilaterale-coop.html"@de ;
          rdfs:label "Sicht"@de ,
                     "View" .


###  https://w3id.org/i40/sto#Standard
sto:Standard rdf:type owl:Class ;
             rdfs:isDefinedBy <https://w3id.org/i40/sto/#Standard> ;
             skos:definition "Standards that are used to describe the actual I4.0 Asset. Defined in the STO ontology"@en ;
             skos:prefLabel "Standard"@en .


###  https://w3id.org/i40/rami#Business
rami:Business rdf:type owl:NamedIndividual .


###  https://w3id.org/i40/rami#BusinessView
rami:BusinessView rdf:type owl:NamedIndividual ,
                           rami:View ;
                  rdfs:label """Allows judging on the business suitability and performance of an asset.
Examples: Manufacturer name, prices, terms of delivery, order codes.”"""@en ,
                             "Business"@en .


###  https://w3id.org/i40/rami#Communication
rami:Communication rdf:type owl:NamedIndividual .


###  https://w3id.org/i40/rami#Confirmation
rami:Confirmation rdf:type owl:NamedIndividual ;
                  rdfs:comment "For responses to requests that describe the capability of an asset."@en ;
                  rdfs:label "Confirmation"@en .


###  https://w3id.org/i40/rami#ConnectedWorld
rami:ConnectedWorld rdf:type owl:NamedIndividual ,
                             rami:HierarchyLevelDimension ;
                    skos:definition "Connected World hierarchy level of RAMI 4.0. Group of Factories, and the collaboration with external engineering firms, component suppliers and customers"@en ;
                    skos:prefLabel "Connected World"@en .


###  https://w3id.org/i40/rami#Construction
rami:Construction rdf:type owl:NamedIndividual ,
                           rami:View ;
                  rdfs:comment """Applies to the constructive deployment of the asset for selection, design of building structure or assemblies ...
Contains a structure classification system pursuant to IEC 81346.
Applies to properties in respect of:
1) physical dimensions and regarding start, processing and output values of the component.
2) modular view of subcomponents or a asset structure and 3) inputs and outputs of different signal types."""@en ;
                  rdfs:label "Construction"@en .


###  https://w3id.org/i40/rami#ControlDevice
rami:ControlDevice rdf:type owl:NamedIndividual ,
                            rami:HierarchyLevelDimension ;
                   skos:definition "Control Device hierarchy level of RAMI 4.0"@en ;
                   skos:prefLabel "Control Device"@en .


###  https://w3id.org/i40/rami#Enterprise
rami:Enterprise rdf:type owl:NamedIndividual ,
                         rami:HierarchyLevelDimension ;
                skos:definition "Enterprise hierarchy level of RAMI 4.0"@en ;
                skos:prefLabel "Enterprise"@en .


###  https://w3id.org/i40/rami#FieldDevice
rami:FieldDevice rdf:type owl:NamedIndividual ,
                          rami:HierarchyLevelDimension ;
                 skos:definition "Field Device hierarchy level of RAMI 4.0"@en ;
                 skos:prefLabel "Field Device"@en .


###  https://w3id.org/i40/rami#Function
rami:Function rdf:type owl:NamedIndividual ,
                       rami:View ;
              rdfs:comment """Makes statements on the function pursuant to IEC 81346 and on the function of the asset components.
Includes skills such as interpretation (?), commissioning, calculation (?) or diagnosis functions of the asset.
Examples: current imbalance detection ability"""@en ;
              rdfs:label "Function"@en .


###  https://w3id.org/i40/rami#Functional
rami:Functional rdf:type owl:NamedIndividual .



###  https://w3id.org/i40/rami#Information
rami:Information rdf:type owl:NamedIndividual .


###  https://w3id.org/i40/rami#Instance
rami:Instance rdf:type owl:NamedIndividual ,
                       rami:AssetKind ;
              rdfs:comment "The RAMI4.0 model [3] features one, generalized life-cycle axis, which was derived from IEC 62890. The basic idea is to distinguish for all assets within Industrie 4.0 between possible types and instance. This makes it possible to apply the type/instance distinction for all elements such as material type/material instance, product type/product instance, machine type/ machine instance and more. Business related information will be handled on the 'Business' layer of the RAMI4.0 model, as well, covering also order details and workflows, again with types/ instances."@en ;
              rdfs:label "Instance"@en ;
              skos:altLabel "Asset Instance"@en .


###  https://w3id.org/i40/rami#Integration
rami:Integration rdf:type owl:NamedIndividual .


###  https://w3id.org/i40/rami#Location
rami:Location rdf:type owl:NamedIndividual ,
                       rami:View ;
              rdfs:comment """Describes the capabilities of the asset regarding environmental conditions and positions of the asset.
Describes the relationships capabilities of the asset or its parts or inputs and outputs with the linked assets.
Examples: Degree of protection, working temperature, EMC characteristics, rated voltage, rated ultimate  
short-circuit breaking capacity."""@en ;
              rdfs:label "Location"@en .




###  https://w3id.org/i40/rami#Measurement
rami:Measurement rdf:type owl:NamedIndividual ;
                 rdfs:comment "If a measured or actual value is provided"@en ;
                 rdfs:label "Measurement"@en .


###  https://w3id.org/i40/rami#Performance
rami:Performance rdf:type owl:NamedIndividual ,
                          rami:View ;
                 rdfs:comment "Describes performance and behavioural characteristics. Examples: To allow a performance assessment and virtual commissioning of an overall system. Example: instantaneous short-circuit current setting"@en ;
                 rdfs:label "Performance"@en .


###  https://w3id.org/i40/rami#Product
rami:Product rdf:type owl:NamedIndividual ,
                      rami:HierarchyLevelDimension ;
             skos:definition "Product hierarchy level of RAMI 4.0"@en ;
             skos:prefLabel "Product"@en .


###  https://w3id.org/i40/rami#Requirement
rami:Requirement rdf:type owl:NamedIndividual ;
                 rdfs:comment "For requests that are to be confirmed or rejected"@en ;
                 rdfs:label "Requirement"@en .


###  https://w3id.org/i40/rami#Station
rami:Station rdf:type owl:NamedIndividual ,
                      rami:HierarchyLevelDimension ;
             skos:definition "Station hierarchy level of RAMI 4.0"@en ;
             skos:prefLabel "Station"@en .


###  https://w3id.org/i40/rami#WorkCenter
rami:WorkCenter rdf:type owl:NamedIndividual ,
                         rami:HierarchyLevelDimension ;
                skos:definition "Work Center hierarchy level of RAMI 4.0"@en ;
                skos:prefLabel "Work Center"@en .


[ rdf:type owl:AllDisjointClasses ;
  owl:members ( rami:HierarchyLevelDimension
                rami:ITLayerDimension
                rami:LifeCycleDimension
              )
] .


###  https://w3id.org/i40/rami#hasRAMIHierarchyLevel
rami:hasRAMIHierarchyLevel rdf:type owl:ObjectProperty ;
                           rdfs:range rami:HierarchyLevelDimension ;
                           skos:definition "Connects the concepts with the correspondent layer in the RAMI Hierarchy Levels"@en ;
                           skos:prefLabel "has RAMI Hierarchy Level"@en .


###  https://w3id.org/i40/rami#hasRAMILifeCycleLayer
rami:hasRAMILifeCycleLayer rdf:type owl:ObjectProperty ;
                           skos:definition "Connects the concepts with the correspondent layer in the RAMI Hierarchy Levels"@en ;
                           skos:prefLabel "has RAMI Life Cycle Layer" .



###  https://w3id.org/i40/rami#internalInformationPoint
rami:internalInformationPoint rdf:type owl:ObjectProperty ;
                              rdfs:comment "References to submodels defining information used by security access permission rules."@en ;
                              rdfs:label "internal InformationPoint"^^xsd:string .

###  https://w3id.org/i40/rami#kindOfPermission
rami:kindOfPermission rdf:type owl:ObjectProperty ;
                      rdfs:comment """Description of the kind of permission. Possible kind of permission also include the denial of the permission.
Values:
􏰀 allow
􏰀 deny
􏰀 not applicable
􏰀 undefined"""@en ;
                      rdfs:label "kind of permission"^^xsd:string .


###  https://w3id.org/i40/rami#permissionsPerObject
rami:permissionsPerObject rdf:type owl:ObjectProperty ;
                          rdfs:comment "Set of object-permission pairs that define the permissions per object within the access permission rule."@en ;
                          rdfs:label "permissionsPerObject"^^xsd:string .


###  https://w3id.org/i40/rami#security
rami:security rdf:type owl:ObjectProperty ;
              rdfs:range rami:Security ;
              rdfs:comment "Definition of the security relevant aspects of the AAS."@en ;
              rdfs:label "security"@en ;
              skos:prefLabel "has security"@en .


###  https://w3id.org/i40/rami#selectablePermissions
rami:selectablePermissions rdf:type owl:ObjectProperty ;
                           rdfs:comment """Reference to a submodel defining which permissions can be assigned to the subjects.
Default: reference to the submodel referenced via
defaultPermissions"""@en ;
                           rdfs:label "selectablePermissions"^^xsd:string .

###  https://w3id.org/i40/rami#LifeCycleDimension
rami:LifeCycleDimension rdf:type owl:Class ;
                        owl:equivalentClass [ rdf:type owl:Class ;
                                              owl:oneOf ( rami:Instance
                                                          rami:Type
                                                        )
                                            ] ;
                        rdfs:subClassOf rami:Dimension ;
                        rdfs:label "RAMI Life Cycle Dimension"@en ;
                        rdfs:seeAlso <http://www.zvei.org/en/subjects/Industry-40/Pages/The-Reference-Architectural-Model-RAMI-40-and-the-Industrie-40-Component.aspx> ,
                                     "http://www.zvei.org/en/subjects/Industry-40/Pages/The-Reference-Architectural-Model-RAMI-40-and-the-Industrie-40-Component.aspx" ;
                        skos:definition "It describes the different Life Cycle and Value Stream layers for the RAMI model according to the IEC 62890 Standard."@en ;
                        skos:prefLabel "RAMI Layer"@en ,
                                       "RAMI Life Cycle Dimension"@en .

###  https://w3id.org/i40/rami#hasValue
rami:hasValue rdf:type owl:DatatypeProperty ;
              rdfs:subPropertyOf owl:topDataProperty ;
			  rdfs:comment """Points to the current value that can be specified through an instanced submodel (for 
instance in station 2) or through the asset."""@en ;
              rdfs:label "has SubmodelElement value"^^xsd:string ;
              rdfs:comment "Submodel element contained in the collection."@en ;
              rdfs:label "has Value"^^xsd:string .




###  https://w3id.org/i40/rami#Manifest
rami:Manifest rdf:type owl:Class ;
              rdfs:label "Manifest"@en .



###  https://w3id.org/i40/rami#hasManifest
rami:hasManifest rdf:type owl:ObjectProperty ;
                 rdfs:domain rami:AdminShell ;
                 rdfs:range rami:Manifest ;
                 rdfs:label "has Manifest"@en .


###  https://w3id.org/i40/rami#selectableSubjectAttributes
rami:selectableSubjectAttributes rdf:type owl:ObjectProperty ;
                                 rdfs:comment """Reference to a submodel defining the authenticated subjects that are configured for the AAS. They are selectable by the access permission rules to assign permissions to the subjects.
Default: reference to the submodel referenced via defaultSubjectAttributes."""@en ;
                                 rdfs:label "selectableSubjectAttributes"^^xsd:string .

###  http://www.w3.org/ns/prov#wasGeneratedBy
prov:wasGeneratedBy rdf:type owl:ObjectProperty ;
                    rdfs:domain rami:Submodel ;
                    skos:definition "Agent (Person or Software) that generates the Asset related data. It correspond to the Creator concept of OMM"@en ;
                    skos:prefLabel "was Generated By"@en .

###  https://w3id.org/i40/rami#hasAssetId
rami:hasAssetId rdf:type owl:DatatypeProperty ;
                rdfs:subPropertyOf dcterms:identifier ;
                rdf:type owl:FunctionalProperty ;
                rdfs:domain rami:Asset ;
                rdfs:range xsd:string ;
                rdfs:comment "Unique identification for the Asset."@en ;
                rdfs:label "has Asset Id"@en ;
                owl:deprecated "true"^^xsd:boolean .


###  https://w3id.org/i40/rami#hasAdminShellId
rami:hasAdminShellId rdf:type owl:DatatypeProperty ;
                     rdfs:subPropertyOf dcterms:identifier ;
                     rdf:type owl:FunctionalProperty ;
                     rdfs:domain rami:AdminShell ;
                     rdfs:range xsd:string ;
                     owl:deprecated "true"^^xsd:boolean ;
                     skos:definition "Unique Identification of the Administration Shell"@en ;
                     skos:prefLabel "has AdminShell Id"@en .



###  https://w3id.org/i40/rami#usesStandard
rami:usesStandard rdf:type owl:ObjectProperty ;
                  rdfs:range sto:Standard ;
                  rdfs:comment "Points to the standards describing the functions attached to the asset via the administration shell"@en ;
                  rdfs:label "uses Standard"@en .

###  https://w3id.org/i40/rami#hasData
rami:hasData rdf:type owl:ObjectProperty ;
             rdfs:domain rami:AdminShell ;
             rdfs:range rami:Submodel ;
             skos:definition "Connects the AdminShell with the different types of Data for the Object/Entity"@en ;
             skos:note "compare with hasDataSpecification" ;
             skos:prefLabel "has Data"@en .

###  http://www.w3.org/2003/01/geo/wgs84_pos#lat
geo:lat rdf:type owl:DatatypeProperty ;
        rdfs:domain rami:Machine ;
        rdfs:range xsd:decimal ;
        rdfs:comment "The WGS84 altitude of a SpatialThing (decimal meters above the local reference ellipsoid)."@en ;
        rdfs:label "lat"@en .

###  https://w3id.org/i40/rami#subModelID
rami:subModelID rdf:type owl:DatatypeProperty ;
                rdfs:subPropertyOf dcterms:identifier ;
                rdf:type owl:FunctionalProperty ;
                rdfs:domain rami:Submodel ;
                rdfs:range xsd:string ;
                rdfs:comment "Unique Identification for a Submodel"@en ;
                rdfs:label "sub Model Id"^^xsd:string ;
                owl:deprecated "true"^^xsd:boolean .


###  http://www.w3.org/2003/01/geo/wgs84_pos#long
geo:long rdf:type owl:DatatypeProperty ;
         rdfs:domain rami:Machine ;
         rdfs:range xsd:decimal ;
         rdfs:comment "The WGS84 longitude of a SpatialThing (decimal degrees)."@en ;
         rdfs:label "long"@en .


###  https://w3id.org/i40/rami#receivedAtTime
rami:receivedAtTime rdf:type owl:DatatypeProperty ;
                    rdfs:domain rami:AdminShell ;
                    rdfs:range xsd:dateTime ;
                    skos:definition "received Time of an Asset"@en ;
                    skos:prefLabel "received At Time"^^xsd:string .


###  https://w3id.org/i40/rami#recordedTime
rami:recordedTime rdf:type owl:DatatypeProperty ;
                  rdfs:domain rami:Machine ;
                  rdfs:range xsd:dateTime ;
                  rdfs:comment "Recorded Time for a Machine"@en ;
                  skos:prefLabel "recorded Time"^^xsd:string .


###  https://w3id.org/i40/rami#releaseDate
rami:releaseDate rdf:type owl:DatatypeProperty ;
                 rdfs:range xsd:date ;
                 skos:definition "Release Date of the Standard"@en ;
                 skos:prefLabel "Release Date"^^xsd:string .
